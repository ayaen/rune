println ()
println (123,)
println (12, "test")
println (12, ("hello", true))
println (12, ("test", (6.5f64, (~0i1,))))

// indexes
point = (1, 2)
println "point=", point, " point[0]=%u" % point[0], ", point[1]=", point[1]

t = (3*5 + 3 + 5, 3**2 + 5**2)
println t[0], " ", t[1]

// equality
t1 = (1, false)
t2 = (1, false)
println t1, " == ", t2, "? ", t1 == t2
t3 = (1, true)
println t1, " == ", t3, "? ", t1 == t3


